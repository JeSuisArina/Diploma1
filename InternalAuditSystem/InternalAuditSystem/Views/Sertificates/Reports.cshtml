@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Стандарты";
    ViewBag.Pagetitle = "Стандарты";
}


<!DOCTYPE html>
<html>
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>Сотрудники</title>
    <!-- vendor css -->
    <link href="../../Scripts/lib/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="../../Scripts/lib/Ionicons/css/ionicons.css" rel="stylesheet">
    <link href="../../Scripts/lib/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet">
    <link href="../../Scripts/lib/rickshaw/rickshaw.min.css" rel="stylesheet">
    <link href="../../Scripts/lib/morris.js/morris.css" rel="stylesheet">
    <link rel="stylesheet" href="../../Content/katniss.css">
</head>

<body>
    <div class="kt-breadcrumb">
        <nav class="breadcrumb">
            <a class="breadcrumb-item" href="../News/Index">Главная страница</a>
            <span class="breadcrumb-item active">@ViewBag.Pagetitle</span>
        </nav>
    </div><!-- kt-breadcrumb -->
    <div class="kt-mainpanel">
        <div class="kt-pagetitle">
            <h5>Стандарты</h5>
        </div>
        <div class="kt-pagebody">

            <div class="row row-sm mg-t-20">
                <div class="col-lg-6">
                    <div class="card pd-20">
                        <h6 class="tx-12 tx-uppercase tx-info tx-bold mg-b-15">Выданные сертификаты</h6>
                        <div class="d-flex mg-b-10">
                            <div class="bd-r pd-r-10">
                                <label class="tx-12">Сегодня</label>
                                <p class="tx-lato tx-inverse tx-bold">1</p>
                            </div>
                            <div class="bd-r pd-x-10">
                                <label class="tx-12">Эта неделя</label>
                                <p class="tx-lato tx-inverse tx-bold">6</p>
                            </div>
                            <div class="pd-l-10">
                                <label class="tx-12">Этот месяц</label>
                                <p class="tx-lato tx-inverse tx-bold">12</p>
                            </div>
                        </div><!-- d-flex -->
                        <div class="progress mg-b-10">
                            <div class="progress-bar bg-info wd-50p" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">50%</div>
                        </div>
                        <p class="tx-12 mg-b-0">-</p>
                    </div><!-- card -->
                </div><!-- col-6 -->
                <div class="col-lg-6 mg-t-20 mg-lg-t-0">
                    <div class="card pd-20">
                        <h6 class="tx-12 tx-uppercase tx-danger tx-bold mg-b-15">Отклоненные заявки</h6>
                        <div class="d-flex mg-b-10">
                            <div class="bd-r pd-r-10">
                                <label class="tx-12">Сегодня</label>
                                <p class="tx-lato tx-inverse tx-bold">1</p>
                            </div>
                            <div class="bd-r pd-x-10">
                                <label class="tx-12">Эта неделя</label>
                                <p class="tx-lato tx-inverse tx-bold">1</p>
                            </div>
                            <div class="pd-l-10">
                                <label class="tx-12">Этот месяц</label>
                                <p class="tx-lato tx-inverse tx-bold">3</p>
                            </div>
                        </div><!-- d-flex -->
                        <div class="progress mg-b-10">
                            <div class="progress-bar bg-danger wd-10p" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100">10%</div>
                        </div>
                        <p class="tx-12 mg-b-0">-</p>
                    </div><!-- card -->
                </div><!-- col-6 -->
            </div><!-- row -->
            <div class="row row-sm mg-t-50">
                <div class="col-xl-6">
                    <div class="card pd-20 pd-sm-40">
                        <h6 class="card-body-title">Заявки</h6>
                        <p>-</p>
                        <div id="morrisBar1" class="ht-200 ht-sm-300"></div>
                    </div><!-- card -->
                </div><!-- col-6 -->
                </div>
                <script src="../../Scripts/lib/jquery/jquery.js"></script>
                <script src="../../Scripts/lib/popper.js/popper.js"></script>
                <script src="../../Scripts/lib/bootstrap/bootstrap.js"></script>
                <script src="../../Scripts/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
                <script src="../../Scripts/lib/moment/moment.js"></script>
                <script src="../../Scripts/lib/d3/d3.js"></script>
                <script src="../../Scripts/lib/rickshaw/rickshaw.min.js"></script>
                <script src="http://maps.google.com/maps/api/js?key=AIzaSyAEt_DBLTknLexNbTVwbXyq2HSf2UbRBU8"></script>
                <script src="../../Scripts/lib/gmaps/gmaps.js"></script>
                <script src="../../Scripts/lib/chart.js/Chart.js"></script>
                <script src="../../Scripts/lib/raphael/raphael.min.js"></script>
                <script src="../../Scripts/lib/morris.js/morris.js"></script>

                <script src="../../Scripts/js/katniss.js"></script>
                <script src="../../Scripts/js/chart.morris.js"></script>
                <script src="../../Scripts/js/ResizeSensor.js"></script>
                <script src="../../Scripts/js/dashboard.js"></script>

</body>
</html>