@model IEnumerable<AuditControlSystem.Models.Subdivision>

@{
    ViewBag.Title = "Подразделения";
    ViewBag.Pagetitle = "Подразделения";
    ViewBag.Pass = "Главная страница";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubdivisionName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubdivisionName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SubdivisionId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SubdivisionId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SubdivisionId })
        </td>
    </tr>
}

</table>
